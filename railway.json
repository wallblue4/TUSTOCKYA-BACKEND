{
    "build": {
      "builder": "dockerfile"
    },
    "deploy": {
      "startCommand": "python -m uvicorn main_standalone:app --host 0.0.0.0 --port $PORT",
      "healthcheckPath": "/health",
      "healthcheckTimeout": 100,
      "restartPolicyType": "on_failure",
      "restartPolicyMaxRetries": 3
    }
  }